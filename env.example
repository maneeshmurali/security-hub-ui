# AWS Configuration
AWS_REGION=us-east-1

# Database Configuration
DATABASE_URL=postgresql://security_hub:security_hub_password@postgres:5432/security_hub

# S3 Configuration (optional)
S3_BUCKET_NAME=
S3_PREFIX=security-hub-findings/

# Application Configuration
APP_PORT=8000
POLLING_INTERVAL_MINUTES=1440  # 24 hours

# Security Hub Filters
DEFAULT_SEVERITY_FILTER=CRITICAL,HIGH,MEDIUM,LOW,INFORMATIONAL
DEFAULT_STATUS_FILTER=ACTIVE,SUPPRESSED

# Multi-region Processing
ENABLE_MULTI_REGION=true  # Set to false to disable multi-region processing 
