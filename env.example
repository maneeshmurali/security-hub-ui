# AWS Security Hub Findings Application Configuration
# Copy this file to .env and modify the values as needed

# AWS Settings
AWS_REGION=us-east-1

# Database settings
DATABASE_URL=sqlite:///./data/security_hub_findings.db

# S3 Settings (optional - uncomment and configure if needed)
# S3_BUCKET_NAME=your-security-hub-bucket
# S3_PREFIX=security-hub-findings/

# Polling settings
POLLING_INTERVAL_MINUTES=30

# API settings
HOST=0.0.0.0
PORT=8000

# Security Hub filters (comma-separated values)
DEFAULT_SEVERITY_FILTER=CRITICAL,HIGH,MEDIUM,LOW,INFORMATIONAL
DEFAULT_STATUS_FILTER=ACTIVE,SUPPRESSED 